<h1>Listing sub_skills</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Skill</th>
    <th>Parent sub skill</th>
    <th>Skill value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sub_skills.each do |sub_skill| %>
  <tr>
    <td><%= sub_skill.name %></td>
    <td><%= sub_skill.skill.name %></td>
    <td><%= sub_skill.parent_sub_skill_id %></td>
    <td><%= sub_skill.skill_value %></td>
    <td><%= link_to 'Show', sub_skill %></td>
    <td><%= link_to 'Edit', edit_sub_skill_path(sub_skill) %></td>
    <td><%= link_to 'Destroy', sub_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sub skill', new_sub_skill_path %>
