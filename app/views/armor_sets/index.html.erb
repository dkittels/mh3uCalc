<h1>Listing armor_sets</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @armor_sets.each do |armor_set| %>
  <tr>
    <td><%= armor_set.name %></td>
    <td><%= link_to 'Show', armor_set %></td>
    <td><%= link_to 'Edit', edit_armor_set_path(armor_set) %></td>
    <td><%= link_to 'Destroy', armor_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Armor set', new_armor_set_path %>
